{
    "name": "argentcrusade/repository",
    "description": "Repository package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ArgentCrusade\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArgentCrusade\\Repository\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "ArgentCrusade\\Repository\\Providers\\RepositoryServiceProvider"
            ]
        }
    }
}
