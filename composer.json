{
    "name": "argentcrusade/repository",
    "description": "Repository package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/legacy-factories": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "ArgentCrusade\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArgentCrusade\\Repository\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "ArgentCrusade\\Repository\\Providers\\RepositoryServiceProvider"
            ]
        }
    }
}
